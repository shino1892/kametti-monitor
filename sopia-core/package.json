{
  "name": "@sopia-bot/core",
  "version": "4.0.0",
  "description": "SOPIA Core Library",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./extension": {
      "types": "./dist/extension/index.d.ts",
      "import": "./dist/extension/index.js",
      "require": "./dist/extension/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:v2": "tsx test/v2-live-test.ts"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "rimraf": "^6.1.2",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "class-transformer": "^0.5.1",
    "eventemitter2": "^6.4.9",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2",
    "ws": "^8.18.3"
  }
}
